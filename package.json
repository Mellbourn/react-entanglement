{
  "name": "react-entanglement",
  "version": "0.0.1",
  "description": "Spooky action at a distance",
  "main": "index.js",
  "scripts": {
    "build": "webpack --bail",
    "develop": "webpack-dev-server --port 3000 --host 0.0.0.0",
    "dist": "NODE_ENV=production npm run build -- -p",
    "start": "npm run develop",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/",
    "test:unit": "NODE_ENV=test karma start",
    "test:coverage": "SAGUI_COVERAGE=true npm run test:unit",
    "test:unit:watch": "npm run test -- --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pirelenito/react-entanglement.git"
  },
  "keywords": [],
  "author": "Paulo Ragonha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pirelenito/react-entanglement/issues"
  },
  "homepage": "https://github.com/pirelenito/react-entanglement#readme",
  "devDependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "sagui": "^5.0.0-rc.7"
  }
}